then
     echo "It looks like routing service is already running, try killing it first"
     echo "     kill `ps axw|grep 'rtiroutingservice.*PulsePiRS'|awk '{$1=$1}1'|cut -d ' ' -f 1|head -n 1`"
     exit
fi
